<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:catty="http://catty.pink/schema/catty"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
  http://catty.pink/schema/catty
  http://catty.pink/schema/catty.xsd">

  <catty:protocol id="protocol"/>
  <catty:client-config id="client" addresses="127.0.0.1:25001"/>
  <catty:reference protocol="protocol" client-config="client" interface="pink.catty.test.spring.api.SpringAService"/>
  <catty:reference protocol="protocol" client-config="client" interface="pink.catty.test.spring.api.SpringBService"/>
</beans>
